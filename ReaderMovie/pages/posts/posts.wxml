<import src="./post-item/post-item-tem.wxml" />
<view>
  <swiper indicator-dots="true" autoplay="true" interval="3000" indicator-color="#ccc">
    <swiper-item>
      <image src="../../images/book.jpg"></image>
    </swiper-item>
    <swiper-item>
      <image src="../../images/coco.jpg"></image>
    </swiper-item>
    <swiper-item>
      <image src="../../images/party.jpg"></image>
    </swiper-item>
  </swiper>
  <block wx:for="{{post_key}}" wx:for-item="item" wx:key="item">
    <template is="postItem" data="{{item}}"/>
  </block>
</view>